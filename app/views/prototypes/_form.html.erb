<%= form_with(model: @prototype,  local: true ) do |f|%>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :catch_copy %><br />
    <%= f.text_area :catch_copy, class: :form__text %>
  </div>
  <div class="field">
    <%= f.label :concept %><br />
    <%= f.text_area :concept, class: :form__text %>
  </div>
  <div class="field">
    <%= f.label :images %><br />
    <%= f.file_field :images %>
  </div>
  <div class="actions">
    <%= f.submit "保存する", class: :form__btn  %>
  </div>
<% end %>